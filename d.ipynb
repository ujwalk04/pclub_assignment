   "source": [
    "#Assigning random values to the parameters. We have 3 parameters here : a,b,c \n",
    "weights=np.random.randn(3)\n",
    "a=weights[0]\n",
    "b=weights[1]\n",
    "c=weights[2]\n",
    "a,b,c"
   ]
  },
